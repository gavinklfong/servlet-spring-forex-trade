{
  "swagger": "2.0",
  "info": {
    "version": "1.0.0",
    "title": "Swagger Forex Rates"
  },
  "host": "api.exchangeratesapi.io",
  "schemes": [
    "https",
    "http"
  ],
  "paths": {
    "/rates/{id}": {
      "get": {
        "summary": "Get the latest forex rates for the base currency",
        "description": "Returns a list of latest rates",
        "operationId": "getLatestRates",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "Id of Forex Rate",
            "required": false,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "$ref": "#/definitions/RateResponse"
            }
          },
          "400": {
            "description": "Invalid ID supplied"
          },
          "404": {
            "description": "Base Currency not found"
          }
        }
      }
    }
  },
  "definitions": {
    "RateResponse": {
      "type": "object",
      "properties": {
        "rates": {
          "$ref": "#/definitions/Rates"
        },
        "id": {
          "type": "string",
          "example": "GBP"
        },
        "base": {
          "type": "string",
          "example": "GBP"
        },
        "date": {
          "type": "string",
          "format": "date"
        }
      },
      "example": {
        "rates": {
          "USD": 1.34,
          "EUR": 0.8,
          "CAD": 1.7611110473,
          "JPY": 147.9903047569
        },
        "id": "GBP",
        "base": "GBP",
        "date": "2020-01-20"
      }
    },
    "Rates": {
      "type": "string",
      "additionalProperties": {
        "type": "number"
      }
    }
  }
}